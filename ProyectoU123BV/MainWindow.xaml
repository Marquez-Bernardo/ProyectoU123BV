        <TextBox x:Name="txtUser" HorizontalAlignment="Center" Margin="0,151,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <Label Content="Contraseña" HorizontalAlignment="Center" Margin="0,174,0,0" VerticalAlignment="Top" />
        <TextBox x:Name="txtPassword" HorizontalAlignment="Center" Margin="0,205,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <Button Content="Iniciar" HorizontalAlignment="Left" Margin="170,242,0,0" VerticalAlignment="Top" Background="#FF76D455" Width="55" Click="Button_Click"/>

    </Grid>
</Window>
